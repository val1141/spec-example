gliffySoy.macro.dialog.importVisioHelpPanel = function(opt_data, opt_ignored) {
  return '<div class="gliffy-help-text"><span id="gliffy-visio-description-spinner"></span><p id="gliffy-visio-description">' + soy.$$escapeHtml('\u0414\u0438\u0430\u043b\u043e\u0433 \u0438\u043c\u043f\u043e\u0440\u0442\u0430 \u043f\u043e\u0434\u0434\u0435\u0440\u0436\u0438\u0432\u0430\u0435\u0442 \u0444\u0430\u0439\u043b\u044b \u0440\u0438\u0441\u043e\u0432\u0430\u043d\u0438\u044f Visio XML (*.vdx), \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0441\u043e\u0437\u0434\u0430\u043d\u044b \u0432 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0438 Microsoft Visio.') + '</p><h3 id="gliffy-import-visio-prep">' + soy.$$escapeHtml('\u041f\u043e\u0434\u0433\u043e\u0442\u043e\u0432\u043a\u0430 \u0444\u0430\u0439\u043b\u0430 Visio') + '</h3><ol><li>' + soy.$$escapeHtml('\u0412 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0438 Visio \u043e\u0442\u043a\u0440\u043e\u0439\u0442\u0435 \u0444\u0430\u0439\u043b, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u043d\u0443\u0436\u043d\u043e \u0438\u043c\u043f\u043e\u0440\u0442\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0432 Gliffy.') + '</li><li>' + soy.$$escapeHtml('\u0415\u0441\u043b\u0438 \u0432 \u044d\u0442\u043e\u043c \u0444\u0430\u0439\u043b\u0435 \u0441\u043e\u0434\u0435\u0440\u0436\u0438\u0442\u0441\u044f \u043d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0442\u0430\u0431\u043b\u0438\u0446, \u0432\u044b\u0431\u0435\u0440\u0438\u0442\u0435 \u0442\u0443, \u043a\u043e\u0442\u043e\u0440\u0443\u044e \u043d\u0443\u0436\u043d\u043e \u044d\u043a\u0441\u043f\u043e\u0440\u0442\u0438\u0440\u043e\u0432\u0430\u0442\u044c.') + '</li><li>' + soy.$$escapeHtml('\u0412\u044b\u0431\u0435\u0440\u0438\u0442\u0435 \u0444\u0430\u0439\u043b \x3e \u0421\u043e\u0445\u0440\u0430\u043d\u0438\u0442\u044c \u043a\u0430\u043a.') + '</li><li>' + soy.$$escapeHtml('\u0412\u044b\u0431\u0435\u0440\u0438\u0442\u0435 \x22Visio Drawing (*.vsdx)\x22 \u0438\u043b\u0438 \x22XML Drawing (*.vdx)\x22.') + '</li><li>' + soy.$$escapeHtml('\u0421\u043e\u