/* module-key = 'ch.bitvoodoo.confluence.plugins.viewtracker:analytics-macro-browser', location = 'ch/bitvoodoo/confluence/plugins/viewtracker/scripts/reporting/macro/page/report/analytics-date-time-overrides.js' */
define("bitvoodoo/viewtracker/reporting/macro/page/report/date-time-range-overrides",["jquery","bitvoodoo/viewtracker/moment"],function(b,a){var c=function(e,d){this.fieldNames=e;this.defaultDates=d;this.minDate=a("20100914","YYYYMMDD").startOf("day");this.maxDate=a().endOf("day")};c.prototype.PARAMETER_ID_PREFIX="macro-param-";c.prototype.parseDate=function(d){return a(d,"YYYY-MM-DD hh:mm A")};c.prototype.initField=function(j,g,i){var f=this;var e;if(g){e=this.parseDate(g)}else{e=i}var d="YYYY-MM-DD hh:mm A";var h=b("#"+this.PARAMETER_ID_PREFIX+j);h.daterangepicker({autoUpdateInput:false,singleDatePicker:true,timePicker:true,minDate:f.minDate,maxDate:f.maxDate,startDate:e,buttonClasses:["aui-button"],cancelClass:"",linkedCalendars:false,alwaysShowCalendars:true,opens:"right",applyClass:"aui-button aui-button-primary",template:Confluence.Templates.Bitvoodoo.viewtracker.DateRangePicker.dropdownWithTime(),showCustomRangeLabel:false,locale:{format:d,cancelLabel:"Clear"}},function(k,l,m){f.startDate=k;f.endDate=l;f.rangeType=m});h.on("apply.daterangepicker",function(l,k){b(this).val(k.startDate.format(d))});h.on("cancel.daterangepicker",function(){b(this).val("")})};c.prototype.beforeParamsSet=function(e){var d=this;b.each(this.fieldNames,function(g,h){var f="";if(e.hasOwnProperty(h)){f=e[h]}d.initField(h,f,d.defaultDates[h])});return e};return c});require(["bitvoodoo/viewtracker/reporting/macro/page/report/date-time-range-overrides","ajs","bitvoodoo/viewtracker/moment"],function(e,c,a){function d(){return a().subtract(30,"days").startOf("day")}function b(){return a().endOf("day")}c.toInit(function(){c.MacroBrowser.setMacroJsOverride("page-analytics",new e(["startDateTime","endDateTime"],{startDateTime:d(),endDateTime:b()}));c.MacroBrowser.setMacroJsOverride("space-analytics",new e(["startDateTime","endDateTime"],{startDateTime:d(),endDateTime:b()}));c.MacroBrowser.setMacroJsOverride("page-user-report",new e(["startDateTime","endDateTime"],{startDateTime:d(),endDateTime