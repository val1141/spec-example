/* module-key = 'ru.webdl.valign:valign-resources', location = '/js/valign.js' */
function toggleButtons(a){AJS.$("li[id^='webdl-valign-button'] > a").removeClass("active");if(a!=="none"){AJS.$("#webdl-valign-button-"+a+" > a").addClass("active")}}function enableButtons(){AJS.$("li[id^='webdl-valign-button']").removeClass("disabled")}function disableButtons(){AJS.$("li[id^='webdl-valign-button']").addClass("disabled")}function findNodes(){var a=AJS.Rte.getEditor().$('td.confluenceTd[data-mce-selected="1"], th.confluenceTh[data-mce-selected="1"]');if(a.length>0){return a}return getNode()}function getNode(){var b=AJS.$(AJS.Rte.getEditor().selection.getNode());var a=null;if(b.is("td")||b.is("th")){a=b}else{if(b.closest("td.confluenceTd").length>0){a=b.closest("td.confluenceTd")}else{if(b.closest("th.confluenceTh").length>0){a=b.closest("th.confluenceTh")}}}return a}define("ru/webdl/valign",["ajs","jquery"],function(a,b){return{init:function(c){b("#rte-toolbar .toolbar-primary .rte-toolbar-group-justification").after(b("<ul class='aui-buttons'></ul>").addClass("rte-toolbar-group-valign"));c.addButton("webdl-valign-button-top",{title:"",tooltip:"\u0412\u044b\u0440\u043e\u0432\u043d\u0438\u0442\u044c \u043f\u043e \u0432\u0435\u0440\u0445\u043d\u0435\u043c\u0443 \u043a\u0440\u0430\u044e",cmd:"v-top",className:"webdl-valign",icon:"aui-icon aui-icon-small aui-iconfont-editor-align-left",locationGroup:"rte-toolbar-group-valign",weight:0});c.addButton("webdl-valign-button-middle",{title:"",tooltip:"\u0412\u044b\u0440\u043e\u0432\u043d\u0438\u0442\u044c \u043f\u043e \u0446\u0435\u043d\u0442\u0440\u0443",cmd:"v-middle",className:"webdl-valign",icon:"aui-icon aui-icon-small aui-iconfont-editor-align-center",locationGroup:"rte-toolbar-group-valign",weight:0});c.addButton("webdl-valign-button-bottom",{title:"",tooltip:"\u0412\u044b\u0440\u043e\u0432\u043d\u0438\u0442\u044c \u043f\u043e \u043d\u0438\u0436\u043d\u0435\u043c\u0443 \u043a\u0440\u0430\u044e",cmd:"v-bottom",className:"webdl-valign",icon:"aui-icon aui-icon-small aui-iconfont-editor-align-right",locatio