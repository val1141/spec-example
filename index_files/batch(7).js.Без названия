if (typeof Confluence == 'undefined') { var Confluence = {}; }
if (typeof Confluence.Templates == 'undefined') { Confluence.Templates = {}; }
if (typeof Confluence.Templates.Multivote == 'undefined') { Confluence.Templates.Multivote = {}; }


Confluence.Templates.Multivote.table = function(opt_data, opt_ignored) {
  var output = '<table class="confluenceTable"><tbody><tr>';
  var headerList4 = opt_data.headers;
  var headerListLen4 = headerList4.length;
  for (var headerIndex4 = 0; headerIndex4 < headerListLen4; headerIndex4++) {
    var headerData4 = headerList4[headerIndex4];
    output += '<th class="confluenceTh">' + soy.$$escapeHtml(headerData4) + '</th>';
  }
  output += '</tr>';
  var rowList10 = opt_data.rows;
  var rowListLen10 = rowList10.length;
  for (var rowIndex10 = 0; rowIndex10 < rowListLen10; rowIndex10++) {
    var rowData10 = rowList10[rowIndex10];
    output += '<tr>';
    var columnList12 = rowData10;
    var columnListLen12 = columnList12.length;
    for (var columnIndex12 = 0; columnIndex12 < columnListLen12; columnIndex12++) {
      var columnData12 = columnList12[columnIndex12];
      output += '<td class="confluenceTd">' + soy.$$escapeHtml(columnData12) + '</td>';
    }
    output += '</tr>';
  }
  output += '</tbody></table>';
  return output;
};
if (goog.DEBUG) {
  Confluence.Templates.Multivote.table.soyTemplateName = 'Confluence.Templates.Multivote.table';
}


Confluence.Templates.Multivote.exampleMultivote = function(opt_data, opt_ignored) {
  return Confluence.Templates.Multivote.table({headers: ['id', 'Usage', 'Warning'], rows: [[opt_data.id, 'Add or delete columns as you please', 'The first column (\x22id\x22) has to stay']]}) + '<p><br/></p>';
};
if (goog.DEBUG) {
  Confluence.Templates.Multivote.exampleMultivote.soyTemplateName = 'Confluence.Templates.Multivote.exampleMultivote';
}

