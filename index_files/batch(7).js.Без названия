        select.append(AJS.$("<option/>").attr("value", 'wsdynamicselect').html('Dynamic Dropdown (Webservice)'));

        select.append(AJS.$("<option/>").attr("value", 'dynamicmultilabel').html('Dynamic Multi-label'));

