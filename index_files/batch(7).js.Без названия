//  AJS.$('#macro-param-extras').val(sorting);
//  AJS.MacroBrowser.processRequiredParameters();
//}

function setPageRef() {
  var pageId = AJS.$("#paramTable").find('#pageIdParamInput').val();
  AJS.$('#macro-param-extras').val(pageId);
}

function setSmartValues() {
  var pageId = AJS.$("#paramTable").find('#pageIdParamInput').val();
  var formName = AJS.$("#paramTable").find('#formNameParamInput').val();
  var fieldName = AJS.$("#paramTable").find('#fieldNameParamInput').val();
  var checked = AJS.$("#paramTable").find('#fieldNameParamInputAsValue').is(':checked');
  var sorting = AJS.$("#paramTable").find('#fieldNameParamInputSorting').val();
  var labelFieldName = AJS.$("#paramTable").find('#fieldNameParamInputLabelFieldName').val();

  if (pageId !== '' && formName !== '' && fieldName !== '') {
    AJS.$('#macro-param-values').val('[' + pageId + '|' + formName + '|' + fieldName + '|' + checked + '|' + sorting + '|' + labelFieldName + ']');
  }
  AJS.MacroBrowser.processRequiredParameters();
}

function setDynamicExtras() {
  var pageId = AJS.$("#paramTable").find('#pageIdParamInput').val();
  var formName = AJS.$("#paramTable").find('#formNameParamInput').val();
  var fieldName = AJS.$("#paramTable").find('#fieldNameParamInput').val();
  var checked = AJS.$("#paramTable").find('#fieldNameParamInputAsValue').is(':checked');
  var sorting = AJS.$("#paramTable").find('#fieldNameParamInputSorting').val();

  if (pageId !== '' && formName !== '' && fieldName !== '') {
    AJS.$('#macro-param-values').val('[' + pageId + '|' + formName + '|' + fieldName + '|' + checked + '|' + sorting + ']');
  }

  var trackChangesE = AJS.$("#macro-param-type-values").find('#trackChanges');
  var trackChanges = (trackChangesE ? trackChangesE.is(':checked') : '');

  var params = trackChanges + '[';
  AJS.$("#paramTable tr.values_toStore").each(function () {
    params = params + AJS.$(this).find('.i_macro-id-input').val() + '=' + AJS.$(this).find('.i_macro-value-input').val() + '|';
  });
  params = params + ']';

  AJS.$('#macro-param-extras').val(params);
  AJS.MacroBrowser.processRequiredParameters();
}

function setForAutoNumber(prefix, initial) {
  AJS.$('#macro-param-values').val(prefix + ":" + initial);
  AJS.MacroBrowser.processRequiredParameters();
}

function setCalcExpression(val) {
  AJS.$('#macro-param-extras').val(val);
  AJS.MacroBrowser.processRequiredParameters();
}

function setCalcKey(val) {
  AJS.$('#macro-param-values').val(val);
  AJS.MacroBrowser.processRequiredParameters();
}

function setSeparator(val) {
  AJS.$('#macro-param-separator').val(val);
  AJS.MacroBrowser.processRequiredParameters();
}

function setExtraContext(val) {
  if (val) {
    AJS.$('#showMappingBtn').hide();
  } else {
    AJS.$('#showMappingBtn').show();
  }
  AJS.$('#macro-param-extracontext').val(val);
  AJS.MacroBrowser.processRequiredParameters();
}

function setAsMacroParamValue(v) {
  AJS.$('#macro-param-values').val(v);
  AJS.MacroBrowser.processRequiredParameters();
}

function setAsMacroParamVTrackChanges(v) {
  AJS.$("#macro-param-type-values").find('#trackChanges').val(v);
  AJS.MacroBrowser.processRequiredParameters();
}

function setIdLabelMapping() {
  var mappingForRoot = AJS.$("#macro-param-type-values").find('#forRoot').val();
  var mappingForId = AJS.$("#macro-param-type-values").find('#forId').val();
  var mappingForLabel = AJS.$("#macro-param-type-values").find('#forLabel').val();
  AJS.$('#macro-param-mapping').val(mappingForRoot + '|' + mappingForId + '|' + mappingForLabel);
  AJS.MacroBrowser.processRequiredParameters();
}

function setAsMacroParamTypeAndPattern() {
  var extrasE = AJS.$("#macro-param-type-values").find('#extras');
  var extras = (extrasE ? extrasE.val() : '');
  var extrasPatternE = AJS.$("#macro-param-type-values").find('#extrasPattern');
  var extrasPattern = (extrasPatternE ? extrasPatternE.val() : '');

  var v = extras + ':' + extrasPattern;
  AJS.$('#macro-param-extras').val(v);
  AJS.MacroBrowser.processRequiredParameters();
}

function resetSubParams() {
  AJS.$('#macro-param-values').val('');
  AJS.$('#macro-param-extras').val('');
  AJS.MacroBrowser.processRequiredParameters();
}

function setToggleInlineComments() {
  var enableCommentsE = AJS.$("#macro-param-type-values").find('#i_macro-enableInlineComments');
  var enableComments = (enableCommentsE ? enableCommentsE.is(':checked') : '');
  setSeparator(enableComments);
}

function setToggleShowComments() {
  var enableCommentsE = AJS.$("#macro-param-type-values").find('#i_macro-enableShowComments');
  var enableComments = (enableCommentsE ? enableCommentsE.is(':checked') : '');
  AJS.$('#macro-param-extras').val(enableComments);
}

function setValues() {
  var trackChangesE = AJS.$("#macro-param-type-values").find('#trackChanges');
  var trackChanges = (trackChangesE ? trackChangesE.is(':checked') : '');

  var extrasE = AJS.$("#macro-param-type-values").find('#extras');
  var extras = (extrasE ? extrasE.val() : '');

  var params = trackChanges + '[';
  AJS.$("#paramTable tr.values_toStore").each(function () {
    params = params + AJS.$(this).find('.i_macro-id-input').val() + '=' + AJS.$(this).find('.i_macro-value-input').val() + '|';
  });
  params = params + ']';

  AJS.$('#macro-param-values').val(params);
  AJS.$('#macro-param-extras').val(extras);
  AJS.MacroBrowser.processRequiredParameters();
}

function setCheckBoxState(paramContainer, paramName, fieldName) {
  var v = AJS.$("#" + paramContainer).find('#' + fieldName);
  var val = (v ? v.is(':checked') : '');
  AJS.$('#' + paramName).val(val);
  AJS.MacroBrowser.processRequiredParameters();
}

function setAppLinkService(fieldName) {
  var selectedServiceElement = AJS.$("#macro-param-div-" + fieldName).find('#appLinkServicesList');
  var selectedService = (selectedServiceElement ? selectedServiceElement.val() : '');
  AJS.$('#macro-param-' + fieldName).val(selectedService);
  AJS.MacroBrowser.processRequiredParameters();
}

function setDbConfig(fieldName) {
  var extrasE = AJS.$("#dbConfigList");
  var v = (extrasE ? extrasE.val() : '');
  AJS.$('#' + fieldName).val(v);
  AJS.MacroBrowser.processRequiredParameters();
}

function setWsConfig(fieldName) {
  var extrasE = AJS.$("#wsConfigList");
  var v = (extrasE ? extrasE.val() : '');
  AJS.$('#' + fieldName).val(v);
  AJS.MacroBrowser.processRequiredParameters();
}

function setExtras() {
  var extrasE = AJS.$("#macro-param-type-values").find('#extras');
  var extras = (extrasE ? extrasE.val() : '');
  AJS.$('#macro-param-extras').val(extras);
  AJS.MacroBrowser.processRequiredParameters();
}

function setStatusId(elem) {
  var trow = AJS.$(elem).closest('tr');
  var valVal = AJS.$(trow).find('.i_macro-value-input').val(elem.value);
  AJS.MacroBrowser.processRequiredParameters();
}

function saveStatusEntry(elem) {
  var trow = AJS.$(elem).closest('tr');

  var idVal = AJS.$('.redef_status_cl:checked').val();
  var valVal = AJS.$(trow).find('.i_macro-value-input').val();

  if (idVal != '' && valVal != '') {
    var html = '<tr class="values_toStore">' +
      '<td class="confluenceTd"><span class="aui-lozenge aui-lozenge-' + idVal + '">' + valVal + '</span>' +
      '<input type="hidden" name="i_macro-id-input" class="macro-param-input i_macro-id-input" value="' + idVal + '"/>' +
      '<input type="hidden" name="i_macro-value-input" class="macro-param-input i_macro-value-input" value="' + valVal + '"/>' +
      '<br/><button type="button" class="aui aui-button" onclick="editStatusEntry(this);"><span class="aui-icon aui-icon-small aui-iconfont-edit">Edit</span></button>' +
      '<button type="button" class="aui aui-button" onclick="removeEntry(this);"><span class="aui-icon aui-icon-small aui-iconfont-remove">Remove</span></button>' +
      '</td></tr>';

    AJS.$(trow).replaceWith(html);
    setValues();
  } else {
    AJS.$(trow).replaceWith('');
  }
  AJS.MacroBrowser.processRequiredParameters();
}

function saveDynamicEntry(elem) {
  var trow = AJS.$(elem).closest('tr');
  var valVal = AJS.$(trow).find('.i_macro-value-input').val();

  if (valVal !== '') {
    var html = '<tr class="values_toStore">' +
      '<td class="confluenceTd"><input type="hidden" name="i_macro-id-input" class="macro-param-input i_macro-id-input" value="' + valVal + '"/>' +
      valVal + '<input type="hidden" name="i_macro-value-input" class="macro-param-input i_macro-value-input" value="' + valVal + '"/>' +
      '<br/><button type="button" class="aui aui-button" onclick="editDynamicEntry(this);"><span class="aui-icon aui-icon-small aui-iconfont-edit">Edit</span></button>' +
      '<button type="button" class="aui aui-button" onclick="removeEntry(this);"><span class="aui-icon aui-icon-small aui-iconfont-remove">Remove</span></button>' +
      '</td></tr>';

    AJS.$(trow).replaceWith(html);
    setDynamicExtras();
  } else {
    AJS.$(trow).replaceWith('');
  }
  AJS.MacroBrowser.processRequiredParameters();
}

function saveEntry(elem) {
  var trow = AJS.$(elem).closest('tr');
  var idVal = AJS.$(trow).find('.i_macro-id-input').val();
  var valVal = AJS.$(trow).find('.i_macro-value-input').val();

  if (idVal != '' && valVal != '') {
    var html = '<tr class="values_toStore confluenceTr">' +
      '<td class="confluenceTd">' + idVal + '<input type="hidden" name="i_macro-id-input" class="macro-param-input i_macro-id-input" value="' + idVal + '"/></td>' +
      '<td class="confluenceTd">' + valVal + '<input type="hidden" name="i_macro-value-input" class="macro-param-input i_macro-value-input" value="' + valVal + '"/>' +
      '<br/><button type="button" class="aui aui-button" onclick="editEntry(this);"><span class="aui-icon aui-icon-small aui-iconfont-edit">Edit</span></button>' +
      '<button type="button" class="aui aui-button" onclick="removeEntry(this);"><span class="aui-icon aui-icon-small aui-iconfont-remove">Remove</span></button>' +
      '</td></tr>';

    AJS.$(trow).replaceWith(html);

    setValues();
  } else {
    AJS.$(trow).replaceWith('');
  }
  AJS.MacroBrowser.processRequiredParameters();
}

function editDynamicEntry(elem) {
  AJS.$('.cf_saveEntry').each(function () {
    AJS.$(this).click();
  });

  var trow = AJS.$(elem).closest('tr');
  var idVal = AJS.$(trow).find('.i_macro-id-input').val();
  var valVal = AJS.$(trow).find('.i_macro-value-input').val();

  addOrModifyDynamicEntry(idVal, valVal, trow);
  AJS.$(trow).find('.i_macro-id-input').focus();
  AJS.MacroBrowser.processRequiredParameters();
}

function editEntry(elem) {
  AJS.$('.cf_saveEntry').each(function () {
    AJS.$(this).click();
  });

  var trow = AJS.$(elem).closest('tr');
  var idVal = AJS.$(trow).find('.i_macro-id-input').val();
  var valVal = AJS.$(trow).find('.i_macro-value-input').val();

  addOrModifyEntry(idVal, valVal, trow);
  AJS.$(trow).find('.i_macro-id-input').focus();
  AJS.MacroBrowser.processRequiredParameters();
}

function editStatusEntry(elem) {
  AJS.$('.cf_saveEntry').each(function () {
    AJS.$(this).click();
  });

  var trow = AJS.$(elem).closest('tr');
  var idVal = AJS.$(trow).find('.i_macro-id-input').val();
  var valVal = AJS.$(trow).find('.i_macro-value-input').val();

  addOrModifyStatusEntry(idVal, valVal, trow);
  AJS.MacroBrowser.processRequiredParameters();
}

function removeEntry(elem) {
  if (window.confirm("Are you sure you want to remove it?")) {
    var trow = AJS.$(elem).closest('tr');
    trow.remove();
    setValues();
  }
}

function removeDynamicEntry(elem) {
  if (window.confirm("Are you sure you want to remove it?")) {
    var trow = AJS.$(elem).closest('tr');
    trow.remove();
    setDynamicExtras();
  }
}

function removeClassifierEntry(elem) {
  if (window.confirm("Are you sure you want to remove it?")) {
    var trow = AJS.$(elem).closest('tr');
    trow.remove();
    setClassifierValues();
  }
}

function lookupPages(element, onItemSelected) {
  if (element != null) {
    var xhr;

    element.change(function(){
      if (AJS.$(element).val() === '') {
        onItemSelected('');
      }
    });

    element.confiFormsAutoComplete({
      minChars: 2,
      cache: false,
      onSelect: function (e, v, item) {
        var selectedPageId = item.attr('data-internal');
        onItemSelected(selectedPageId);
        try {
          e.preventDefault();
        } catch (e) {
        }
        return false;
      },
      renderItem: function (item, search) {
        //search = search.replace(/[-\/\\^$*+?.()|[\]{}]/g, '\\$&');
        //var re = new RegExp("(" + search.split(' ').join('|') + ")", "gi");
        var encodedLabel = encodeHTML1(item.label);
        return "<a class='autocomplete-suggestion text' style='display: block;' data-internal='" + item.id + "' data-val='" + encodedLabel + "'>"
          + "<span data-internal='" + item.id + "' data-val='" + encodedLabel + "' style='padding-left: 10px;font-size: inherit;padding-right: 0; cursor: pointer'>" + encodedLabel + "</span></a>";
      },
      source: function (term, response) {
        try {
          xhr.abort();
        } catch (e) {
        }

        xhr = AJS.$.getJSON(AJS.params.contextPath + "/rest/prototype/1/search/name.json?max-results=20&pageSize=25&type=page%2Cblogpost", {query: "" + term}, function (rawData) {
          var data = AJS.$.parseJSON(JSON.stringify(rawData));
          var r = new Array();
          var idx = 0;
          if (data.group && data.group.length > 0) {
            var jsonResult = data.group[0].result;
            for (var key = 0; key < jsonResult.length; key++) {
              r[idx++] = {
                "id": jsonResult[key].id,
                "label": jsonResult[key].space.key + ':' + jsonResult[key].title + ' (' + jsonResult[key].type + ')'
              };
            }
          }
          if (data.group && data.group.length > 1) {
            var jsonResult = data.group[1].result;
            for (var key = 0; key < jsonResult.length; key++) {
              r[idx++] = {
                "id": jsonResult[key].id,
                "label": jsonResult[key].space.key + ':' + jsonResult[key].title + ' (' + jsonResult[key].type + ')'
              };
            }
          }
          response(r);
        });
      }
    });
  }
}

AJS.MacroBrowser.setMacroJsOverride("confiform-ifttt", {
  fields: {
    "string": function (param, options) {
      if (param.name == "action") {
        var paramDiv = AJS.$(Confluence.Templates.MacroBrowser.macroParameterSelect());
        var select = AJS.$("select", paramDiv);
        select.append(AJS.$("<option/>").attr("value", '').html(''));
        select.append(AJS.$("<option/>").attr("value", 'Add Attachment Label').html('Add Attachment Label'));
        select.append(AJS.$("<option/>").attr("value", 'Add Page Label').html('Add Page Label'));
        select.append(AJS.$("<option/>").attr("value", 'Add Page Watcher').html('Add Page Watcher'));
        select.append(AJS.$("<option/>").attr("value", 'AppLink service').html('AppLink service'));
        select.append(AJS.$("<option/>").attr("value", 'Audit').html('Audit'));
        select.append(AJS.$("<option/>").attr("value", 'Copy Attachment').html('Copy Attachment'));
        select.append(AJS.$("<option/>").attr("value", 'Copy ConfiForms Data').html('Copy ConfiForms Data'));
        select.append(AJS.$("<option/>").attr("value", 'Copy ConfiForms Data to DB').html('Copy ConfiForms Data to DB'));
        select.append(AJS.$("<option/>").attr("value", 'Copy Page').html('Copy Page'));
        select.append(AJS.$("<option/>").attr("value", 'Create Attachment').html('Create Attachment'));
        select.append(AJS.$("<option/>").attr("value", 'Create BlogPost').html('Create BlogPost'));
        select.append(AJS.$("<option/>").attr("value", 'Create Comment').html('Create Comment'));
        select.append(AJS.$("<option/>").attr("value", 'Create ConfiForms Entry').html('Create / Update ConfiForms Entry'));
        select.append(AJS.$("<option/>").attr("value", 'Create Insight Object').html('Create Insight Object'));
        select.append(AJS.$("<option/>").attr("value", 'Create JIRA Issue').html('Create JIRA Issue'));
        select.append(AJS.$("<option/>").attr("value", 'Create Page').html('Create Page'));
        // select.append(AJS.$("<option/>").attr("value", 'Create Task').html('Create Confluence Task'));
        select.append(AJS.$("<option/>").attr("value", 'Delete Attachment').html('Delete Attachment'));
        select.append(AJS.$("<option/>").attr("value", 'Delete ConfiForms Entry').html('Delete ConfiForms Entry'));
        select.append(AJS.$("<option/>").attr("value", 'Delete Page').html('Delete Page'));
        select.append(AJS.$("<option/>").attr("value", 'Force Page Re-index').html('Force Page Re-index'));
        select.append(AJS.$("<option/>").attr("value", 'Generate next number').html('Generate next number (experimental)'));
        select.append(AJS.$("<option/>").attr("value", 'Make request').html('Make HTTP Request (deprecated)'));
        select.append(AJS.$("<option/>").attr("value", 'Move Attachment').html('Move Attachment'));
        select.append(AJS.$("<option/>").attr("value", 'Move Page').html('Move Page'));
        select.append(AJS.$("<option/>").attr("value", 'Redirect to URL').html('Redirect to URL'));
        select.append(AJS.$("<option/>").attr("value", 'Remove Attachment Label').html('Remove Attachment Label'));
        select.append(AJS.$("<option/>").attr("value", 'Remove Page Label').html('Remove Page Label'));
        select.append(AJS.$("<option/>").attr("value", 'Remove Page Edit Restrictions').html('Remove Page Edit Restrictions'));
        select.append(AJS.$("<option/>").attr("value", 'Remove Page View Restrictions').html('Remove Page View Restrictions'));
        select.append(AJS.$("<option/>").attr("value", 'Remove Page Watcher').html('Remove Page Watcher'));
        select.append(AJS.$("<option/>").attr("value", 'Rename Page').html('Rename Page'));
        select.append(AJS.$("<option/>").attr("value", 'Restrict Page Edit').html('Restrict Page Edit'));
        select.append(AJS.$("<option/>").attr("value", 'Restrict Page View').html('Restrict Page View'));
        select.append(AJS.$("<option/>").attr("value", 'Render (execute) macro').html('Render (execute) macro'));
        select.append(AJS.$("<option/>").attr("value", 'Run JavaScript').html('Run JavaScript'));
        select.append(AJS.$("<option/>").attr("value", 'Send Email').html('Send Email'));
        select.append(AJS.$("<option/>").attr("value", 'Send Notification').html('Send Notification'));
        select.append(AJS.$("<option/>").attr("value", 'Set Page Revision').html('Set Page Revision'));
        select.append(AJS.$("<option/>").attr("value", 'Update JIRA Issue').html('Update JIRA Issue'));
        select.append(AJS.$("<option/>").attr("value", 'Update ConfiForms entries by filter').html('Update ConfiForms entries by filter'));
        select.append(AJS.$("<option/>").attr("value", 'WebService Request').html('WebService Request'));

        select.change(function () {
          showLabels(AJS.$(this).val());
        });

        select.css('width', '100%');
        return AJS.MacroBrowser.IFTTTMacroField(paramDiv, select, options);
      } else if ((param.name == "extras") || (param.name == "extras2")
        || (param.name == "extras3")
        || (param.name == "additionalContext")
        || (param.name == "extras4") || (param.name == "extras5")) {
        var paramDiv = AJS.$(Confluence.Templates.MacroBrowser.macroParameter());
        var input = AJS.$("input", paramDiv);
        return AJS.MacroBrowser.IFTTTMacroField(paramDiv, input, options);
      } else {
        var paramDiv = $(Confluence.Templates.MacroBrowser.macroParameter());
        var input = $("input", paramDiv);
        if (param.required) {
          input.keyup(AJS.MacroBrowser.processRequiredParameters);
        }
        return AJS.MacroBrowser.IFTTTMacroField(paramDiv, input, options);
      }
    }
  }
});

AJS.MacroBrowser.IFTTTMacroField = function (paramDiv, input, options) {
  options = options || {};

  var setValue = options.setValue || function (value) {
      input.val(value);
      showLabels(AJS.$('#macro-param-action').val());
    };

  var getValue = options.getValue || function () {
      return input.val();
    };

  input.change(options.onchange || AJS.MacroBrowser.paramChanged);

  return {
    paramDiv: paramDiv,
    input: input,
    setValue: setValue,
    getValue: getValue
  };
};

function showLabels(choice) {
  AJS.$('#extraDetails').remove();

  var titleLabel = 'Subject for email message';
  var whoLabel = 'Recipients';
  var extrasLabel = 'Sender';
  var extras2Label = 'Labels';
  var extras3Label = 'Flatten entries';
  var extras4Label = 'Bypass field restrictions and validation rules';
  var extras5Label = 'All IFTTT rules should be used (including lifecycle rules, such as: create/modify/copy ConfiForms entries)';
  var additionalContextLabel = 'Set parameter name (multipart file upload)';

  var standardTitleText = 'Could be constructed dynamically. You can reference record owner as [owner]; record modifier, as [modifier]; any record field as [entry.FIELD_NAME], you can reference page watchers as [watchers], you can reference page contributors as [contributors]';
  var standardWhoText = 'Multiple values should be given as comma separated list. Record owner could be referenced as [owner], Record modifier, as [modifier]. To reference field value use: [entry.FIELD_NAME]';
  var standardExtrasText = 'Accepts username or email address. Leave blank if you want current user to be used as sender. You can use references to entry values via [entry.FIELD_NAME]';
  var standardExtras2Text = 'Comma separated list of labels to add to created page. You can use references to entry values via [entry.FIELD_NAME]';
  var standardExtras3Text = 'Flatten entries before copying (when form has multi-select values)';
  var standardExtras4Text = 'When checked, field definition edit restrictions will be ignored';
  var standardExtras5Text = 'When checked, all IFTTTs will be called (including the ones to create/copy data). This assumes your "Stop events propagation" is not set. Important! This might cause infinite loops!';
  var additionalContextText = 'Set when you are configuring the file uploads (using ConfiForms Field macros inside this IFTTT and want to use multi-part form data method to upload). Leave empty then the file should be uploaded as a stream';

  AJS.$('#macro-param-div-title').show();
  AJS.$('#macro-param-div-who').show();
  AJS.$('#macro-param-div-extras').show();
  AJS.$('#macro-param-div-extras2').hide();
  AJS.$('#macro-param-div-extras3').hide();
  AJS.$('#macro-param-div-extras4').hide();
  AJS.$('#macro-param-div-extras5').hide();
  AJS.$('#macro-param-div-additionalContext').hide();
  AJS.$('#macro-param-extras').show();

  // remove autocomplete
  try {
    AJS.$('#macro-param-div-extras').find('#macro-param-extras').destroy();
  } catch (e) {

  }

  if (choice == null || '' === choice) {
    AJS.$('#macro-param-div-title').hide();
    AJS.$('#macro-param-div-who').hide();
    AJS.$('#macro-param-div-extras').hide();
    AJS.$('#macro-param-div-extras2').hide();
    AJS.$('#macro-param-div-extras3').hide();
    AJS.$('#macro-param-div-additionalContext').hide();
  }

  if ('Create Comment' === choice) {
    extrasLabel = 'Page';
    standardExtrasText = 'You (and the users who will use the form) must have edit permissions on this page. Leave blank if current page should be used as parent page. To reference field value use: [entry.FIELD_NAME]. Must resolve into spaceKey:pageTitle or in pageId value. Leaving this parameter blank means current page.';

    lookupPages(AJS.$('#macro-param-div-extras').find('#macro-param-extras'),
      function (pageId) {
        AJS.$('#macro-param-div-extras').find('#macro-param-extras').val(pageId).trigger("change");
        AJS.MacroBrowser.processRequiredParameters();
      });

    AJS.$('#macro-param-div-extras').show();
    AJS.$('#macro-param-div-who').hide();
    AJS.$('#macro-param-div-title').hide();
    AJS.$('#macro-param-div-extras2').hide();
    AJS.$('#macro-param-div-extras3').hide();
  }

  if ('Create Page' === choice) {
    titleLabel = 'Page Title';
    whoLabel = 'Page editors (comma separated list of users/usergroups)';
    extrasLabel = 'Parent page';
    standardExtrasText = 'You (and the users who will use the form) must have edit permissions on this page. Leave blank if current page should be used as parent page. To reference field value use: [entry.FIELD_NAME]. Must resolve into spaceKey:pageTitle or in pageId value. Leaving this parameter blank means current page.';

    lookupPages(AJS.$('#macro-param-div-extras').find('#macro-param-extras'),
      function (pageId) {
        AJS.$('#macro-param-div-extras').find('#macro-param-extras').val(pageId).trigger("change");
        AJS.MacroBrowser.processRequiredParameters();
      });

    extras3Label = 'Skip creation if the page with a given title and in a given Confluence space already exists';
    standardExtras3Text = '';

    AJS.$('#macro-param-div-extras').show();
    AJS.$('#macro-param-div-extras2').show();
    AJS.$('#macro-param-div-extras3').show();
  }

  if ('Delete Page' === choice) {
    // titleLabel = 'Page title';
    whoLabel = '';
    extrasLabel = 'Page to DELETE';
    standardExtrasText = 'You (and the users who will use the form) must have edit permissions on this page. To reference field value use: [entry.FIELD_NAME]. Must resolve into spaceKey:pageTitle or in pageId value. Leaving this parameter blank means current page';

    lookupPages(AJS.$('#macro-param-div-extras').find('#macro-param-extras'),
      function (pageId) {
        AJS.$('#macro-param-div-extras').find('#macro-param-extras').val(pageId).trigger("change");
        AJS.MacroBrowser.processRequiredParameters();
      });

    AJS.$('#macro-param-div-extras').show();
    AJS.$('#macro-param-div-extras2').hide();
    AJS.$('#macro-param-div-title').hide();
    AJS.$('#macro-param-div-who').hide();
  }
  if ('Move Page' === choice) {
    titleLabel = 'Page title or page ID to move';
    whoLabel = 'Page editors (comma separated list of users/usergroups)';
    extrasLabel = 'Parent page';
    standardTitleText = 'You (and the users who will use the form) must have edit permissions on this page. To reference field value use: [entry.FIELD_NAME]. Must resolve into spaceKey:pageTitle or in pageId value. Leaving this parameter blank means current page';
    standardExtrasText = 'You (and the users who will use the form) must have edit permissions on this page. To reference field value use: [entry.FIELD_NAME]. Must resolve into spaceKey:pageTitle or in pageId value. Leaving this parameter blank means current page';

    lookupPages(AJS.$('#macro-param-div-extras').find('#macro-param-extras'),
      function (pageId) {
        AJS.$('#macro-param-div-extras').find('#macro-param-extras').val(pageId).trigger("change");
        AJS.MacroBrowser.processRequiredParameters();
      });

    AJS.$('#macro-param-div-extras').show();
    AJS.$('#macro-param-div-title').show();
    AJS.$('#macro-param-div-who').hide();
    AJS.$('#macro-param-div-extras2').hide();
  }
  if ('Copy Page' === choice) {
    titleLabel = 'Page title or page ID to copy';
    whoLabel = 'Page editors (comma separated list of users/usergroups)';
    extrasLabel = 'Parent page';
    standardTitleText = 'You (and the users who will use the form) must have at least view permissions on this page. To reference field value use: [entry.FIELD_NAME]. Must resolve into spaceKey:pageTitle or in pageId value. Leaving this parameter blank means current page';
    standardExtrasText = 'You (and the users who will use the form) must have edit permissions on this page. To reference field value use: [entry.FIELD_NAME]. Must resolve into spaceKey:pageTitle or in pageId value. Leaving this parameter blank means current page';

    lookupPages(AJS.$('#macro-param-div-extras').find('#macro-param-extras'),
      function (pageId) {
        AJS.$('#macro-param-div-extras').find('#macro-param-extras').val(pageId).trigger("change");
        AJS.MacroBrowser.processRequiredParameters();
      });

    AJS.$('#macro-param-div-extras').show();
    AJS.$('#macro-param-div-title').show();
    AJS.$('#macro-param-div-who').hide();
    AJS.$('#macro-param-div-extras2').hide();

    extras4Label = 'Do not evaluate page body';
    standardExtras4Text = 'By default the page contents will be evaluated against the field values in the current record. Meaning that all the [entry.field_name] notations will be processed and evaluated. You can disable this here';
    AJS.$('#macro-param-div-extras4').show();

    extras3Label = 'Also copy page children';
    standardExtras3Text = '';
    AJS.$('#macro-param-div-extras3').show();
  }
  if ('Force Page Re-index' === choice) {
    titleLabel = ' ';
    whoLabel = ' ';
    extrasLabel = 'Page to re-index';
    standardTitleText = ' ';
    standardExtrasText = 'To reference field value use: [entry.FIELD_NAME]. Must resolve into spaceKey:pageTitle or in pageId value. Leaving this parameter blank means current page';

    lookupPages(AJS.$('#macro-param-div-extras').find('#macro-param-extras'),
      function (pageId) {
        AJS.$('#macro-param-div-extras').find('#macro-param-extras').val(pageId).trigger("change");
        AJS.MacroBrowser.processRequiredParameters();
      });

    AJS.$('#macro-param-div-title').hide();
    AJS.$('#macro-param-div-who').hide();
    AJS.$('#macro-param-div-extras').show();
    AJS.$('#macro-param-div-extras2').hide();
  }
  if ('Render (execute) macro' === choice) {
    titleLabel = ' ';
    whoLabel = ' ';
    extrasLabel = ' ';
    standardTitleText = ' ';
    standardExtrasText = ' ';

    AJS.$('#macro-param-div-title').hide();
    AJS.$('#macro-param-div-who').hide();
    AJS.$('#macro-param-div-extras').hide();
    AJS.$('#macro-param-div-extras2').hide();
  }
  if ('Add Page Watcher' === choice) {
    titleLabel = 'Page watcher(s) to add';
    whoLabel = ' ';
    extrasLabel = 'Page';
    standardTitleText = 'Comma separated list of users or user groups, You can use [entry.FIELD_NAME] to reference fields of current record';
    standardExtrasText = 'To reference field value use: [entry.FIELD_NAME]. Must resolve into spaceKey:pageTitle or in pageId value. Leaving this parameter blank means current page';

    lookupPages(AJS.$('#macro-param-div-extras').find('#macro-param-extras'),
      function (pageId) {
        AJS.$('#macro-param-div-extras').find('#macro-param-extras').val(pageId).trigger("change");
        AJS.MacroBrowser.processRequiredParameters();
      });

    AJS.$('#macro-param-div-title').show();
    AJS.$('#macro-param-div-who').hide();
    AJS.$('#macro-param-div-extras').show();
    AJS.$('#macro-param-div-extras2').hide();
  }
  if ('Remove Page Watcher' === choice) {
    titleLabel = 'Page watcher(s) to remove';
    whoLabel = ' ';
    extrasLabel = 'Page';
    standardTitleText = 'Comma separated list of users or user groups, You can use [entry.FIELD_NAME] to reference fields of current record';
    standardExtrasText = 'To reference field value use: [entry.FIELD_NAME]. Must resolve into spaceKey:pageTitle or in pageId value. Leaving this parameter blank means current page';

    lookupPages(AJS.$('#macro-param-div-extras').find('#macro-param-extras'),
      function (pageId) {
        AJS.$('#macro-param-div-extras').find('#macro-param-extras').val(pageId).trigger("change");
        AJS.MacroBrowser.processRequiredParameters();
      });

    AJS.$('#macro-param-div-title').show();
    AJS.$('#macro-param-div-who').hide();
