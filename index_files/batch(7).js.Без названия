        select.append(AJS.$("<option/>").attr("value", 'hidden').html('Hidden'));
        select.append(AJS.$("<option/>").attr("value", 'hiddenhtml').html('Hidden HTML'));
        select.append(AJS.$("<option/>").attr("value", 'html').html('HTML'));
        select.append(AJS.$("<option/>").attr("value", 'insight').html('Insight Object'));
        select.append(AJS.$("<option/>").attr("value", 'multiinsight').html('Insight Object (multi-select)'));
        select.append(AJS.$("<option/>").attr("value", 'insightselect').html('Insight Objects Dropdown'));
        select.append(AJS.$("<option/>").attr("value", 'persiandate').html('Iranian Calendar'));
        select.append(AJS.$("<option/>").attr("value", 'infopanel').html('Info Panel (readonly)'));
        select.append(AJS.$("<option/>").attr("value", 'ip').html('IP Address'));
        select.append(AJS.$("<option/>").attr("value", 'markdown').html('Markdown Editor'));
        select.append(AJS.$("<option/>").attr("value", 'maskedtext').html('Masked Text'));

        select.append(AJS.$("<option/>").attr("value", 'multi').html('Multi-select'));
        select.append(AJS.$("<option/>").attr("value", 'number').html('Numeric'));
        select.append(AJS.$("<option/>").attr("value", 'numberhidden').html('Numeric (hidden)'));

        select.append(AJS.$("<option/>").attr("value", 'jira').html('Jira Issue'));
        select.append(AJS.$("<option/>").attr("value", 'multijira').html('Jira Issues (multi-select)'));
        select.append(AJS.$("<option/>").attr("value", 'jql').html('Jira JQL'));

        select.append(AJS.$("<option/>").attr("value", 'page').html('Page/Blogpost'));

        select.append(AJS.$("<option/>").attr("value", 'multipage').html('Page/Blogpost (multi-select)'));
        select.append(AJS.$("<option/>").attr("value", 'password').html('Password'));
        select.append(AJS.$("<option/>").attr("value", 'radio_group').html('Radio Group'));
        select.append(AJS.$("<option/>").attr("value", 'read_only').html('Readonly'));
        select.append(AJS.$("<option/>").attr("value", 'section').html('Section'));
        select.append(AJS.$("<option/>").attr("value", 'securitygroup').html('Security Group'));
        select.append(AJS.$("<option/>").attr("value", 'securitygroupmulti').html('Security Group (multi-select)'));
        select.append(AJS.$("<option/>").attr("value", 'simpledate').html('Simple Date'));
        select.append(AJS.$("<option/>").attr("value", 'smartclass').html('Smart Classifier'));
        // select.append(AJS.$("<option/>").attr("value", 'smartconfiquery').html('Smart ConfiForms query (BETA)'));
        select.append(AJS.$("<option/>").attr("value", 'smartselect').html('Smart Dropdown'));

        select.append(AJS.$("<option/>").attr("value", 'smartselect2').html('Smart Dropdown (advanced)'));
        select.append(AJS.$("<option/>").attr("value", 'smartmultiselect').html('Smart Multi-select'));
        select.append(AJS.$("<option/>").attr("value", 'smartmultirow').html('Smart Multi-row'));

        select.append(AJS.$("<option/>").attr("value", 'smartcheckbox').html('Smart Checkbox'));
        select.append(AJS.$("<option/>").attr("value", 'smartradio').html('Smart Radio Group'));

        select.append(AJS.$("<option/>").attr("value", 'spaces').html('Spaces'));

        select.append(AJS.$("<option/>").attr("value", 'status').html('Status'));
        select.append(AJS.$("<option/>").attr("value", 'storageformat').html('Storage Format'));

        select.append(AJS.$("<option/>").attr("value", 'textarea').html('Textarea'));
        select.append(AJS.$("<option/>").attr("value", 'time').html('Time'));
        select.append(AJS.$("<option/>").attr("value", 'timestamp').html('Timestamp (epoch)'));
        select.append(AJS.$("<option/>").attr("value", 'user').html('User'));

        select.append(AJS.$("<option/>").attr("value", 'multiuser').html('User (multi-select)'));

        select.append(AJS.$("<option/>").attr("value", 'version').html('Version'));
        select.append(AJS.$("<option/>").attr("value", 'voting').html('Voting Control'));

        select.append(AJS.$("<option/>").attr("value", 'objectRef').html('Webservice Object Referencing Field'));

        select.append(AJS.$("<option/>").attr("value", 'wsselect').html('Webservice Dropdown'));
        select.append(AJS.$("<option/>").attr("value", 'wsselect2').html('Webservice Dropdown (advanced)'));

        select.append(AJS.$("<option/>").attr("value", 'wsdynamicmultilabel').html('Webservice Dynamic Multi-label'));
        select.append(AJS.$("<option/>").attr("value", 'wsmultiselect').html('Webservice Multi-select'));
        select.append(AJS.$("<option/>").attr("value", 'wscheckbox').html('Webservice Checkbox'));
        select.append(AJS.$("<option/>").attr("value", 'wsradio').html('Webservice Radio Group'));

        select.append(AJS.$("<option/>").attr("value", 'wysiwyg').html('WYSIWYG Simple Editor'));

        select.change(function () {
          showCFParams(AJS.$(this).val());
        });

        select.css('width', '100%');

        select.after('<div id="macro-param-type-values"></div>');

        //AJS.$(select).val("text"); // set text option as default

        //showCFParams(AJS.$(select).val());

        return AJS.MacroBrowser.CFField2(paramDiv, select, options);
      } else if ((param.name === "values") || (param.name === "extras") || (param.name === "separator") || (param.name === "mapping") || (param.name === "extracontext")) {
        var paramDiv = AJS.$(Confluence.Templates.MacroBrowser.macroParameterHidden());
        paramDiv.hide();
        var input = AJS.$("input", paramDiv);
        return AJS.MacroBrowser.Field(paramDiv, input, options);
      } else {
        var paramDiv = $(Confluence.Templates.MacroBrowser.macroParameter());
        var input = $("input", paramDiv);
        if (param.required) {
          input.keyup(AJS.MacroBrowser.processRequiredParameters);
        }
        return AJS.MacroBrowser.Field(paramDiv, input, options);
      }
    }
  }
});

AJS.MacroBrowser.CFField2 = function (paramDiv, input, options) {
  options = options || {};

  var setValue = options.setValue || function (value) {
    if (value == null || value === '') {
      value = 'text';
    }
      input.val(value);
      showCFParams(value);
      //resetSubParams();
    };

  var getValue = options.getValue || function () {
      return input.val();
    };

  input.change(options.onchange || AJS.MacroBrowser.paramChanged);

  return {
    paramDiv: paramDiv,
    input: input,
    setValue: setValue,
    getValue: getValue
  };
};

function showCFParams(val) {
  //if (val == 'checkbox_group' || val == 'radio_group' || val == 'select' || val == 'dynamicselect' || val == 'dynamicmultilabel' || val == 'smartselect' || val == 'smartcheckbox' || val == 'smartradio' || val == 'smartmultiselect' || val == 'smartclass' || val == 'status' || val == 'multi' || val == 'auto' || val == 'simpledate' || val == 'page' || val == 'autopage' || val == 'hidden' || val == 'read_only' || val == 'hiddenhtml' || val == 'user' || val == 'autolink' || val == 'maskedtext' || val == 'currency' || val == 'calc' || val == 'formula' || val == 'enroll' || val == 'voting' || val == 'datetime' || val == 'datetimeinterval' || val == 'multiuser' || val == 'action_button') {
  var paramsDivHtml = "";
  $("#macro-browser-dialog button.ok").prop("disabled", true);
  jQuery.ajax({
    type: "POST",
    async: false,
    url: AJS.params.contextPath + "/plugins/servlet/confiform/params",
    success: function (data) {
      paramsDivHtml = data;
      AJS.$("#macro-param-type-values").replaceWith(paramsDivHtml);

      if (val === 'smartselect' || val === 'smartselect2' || val === 'smartmultiselect' || val === 'smartcheckbox' || val === 'smartradio') {
        setSmartValues();
        AJS.MacroBrowser.processRequiredParameters();

        lookupPages(AJS.$('#macro-param-type-values').find('input[id="pageParamInput"]'),
          function (pageId) {
            AJS.$('#macro-param-type-values').find('input[id="pageIdParamInput"]').val(pageId).trigger("change");
            setSmartValues();
            showCFParams(val);
            AJS.MacroBrowser.processRequiredParameters();
          });
      }
      if (val === 'attachment') {
        setPageRef();
        AJS.MacroBrowser.processRequiredParameters();

        lookupPages(AJS.$('#macro-param-type-values').find('input[id="pageParamInput"]'),
          function (pageId) {
            AJS.$('#macro-param-type-values').find('input[id="pageIdParamInput"]').val(pageId).trigger("change");
            setPageRef();
            AJS.MacroBrowser.processRequiredParameters();
          });
      }

      if (val === 'dynamicselect' || val === 'dynamicmultilabel') {
        setDynamicExtras();
        AJS.MacroBrowser.processRequiredParameters();

        lookupPages(AJS.$('#macro-param-type-values').find('input[id="pageParamInput"]'),
          function (pageId) {
            AJS.$('#macro-param-type-values').find('input[id="pageIdParamInput"]').val(pageId).trigger("change");
            setDynamicExtras();
            showCFParams(val);
            AJS.MacroBrowser.processRequiredParameters();
          });
      }

      if (val === 'smartclass' || val === 'smartmultirow' || val === 'smartconfiquery') {
        setClassifierValues();
        AJS.MacroBrowser.processRequiredParameters();

        lookupPages(AJS.$('#macro-param-type-values').find('input[id="pageParamInput"]'),
          function (pageId) {
            AJS.$('#macro-param-type-values').find('input[id="pageIdParamInput"]').val(pageId).trigger("change");

            AJS.$("#paramTable tr.values_toStore").each(function () {
              AJS.$(this).remove();
            });

            setClassifierValues();
            showCFParams(val);
            AJS.MacroBrowser.processRequiredParameters();
          });
      }

      AJS.MacroBrowser.processRequiredParameters();
    },
    data: jQuery.param({
      params: jQuery('#macro-param-values').val(),
      extras: jQuery('#macro-param-extras').val(),
      extracontext: jQuery('#macro-param-extracontext').val(),
      separator: jQuery('#macro-param-separator').val(),
      mapping: jQuery('#macro-param-mapping').val(),
      pageId: AJS.Meta.get("page-id"),
      type: val
    })
  });
}

function addDynamicNext() {
  AJS.$('.cf_saveEntry').each(function () {
    AJS.$(this).click();
  });
  addOrModifyDynamicEntry('', '', null);
  AJS.$('.i_macro-id-input').focus();
}

function addNext() {
  AJS.$('.cf_saveEntry').each(function () {
    AJS.$(this).click();
  });
  addOrModifyEntry('', '', null);
  AJS.$('.i_macro-id-input').focus();
}

function addNextClassifier() {
  AJS.$('.cf_saveEntry').each(function () {
    AJS.$(this).click();
  });
  if (AJS.$('#fieldNameParamInput').val() && AJS.$('#fieldNameParamInput').val() != '') {
    addOrModifyClassifierEntry(AJS.$('#fieldNameParamInput').val(), null);
  }
}

function addNextStatus() {
  AJS.$('.cf_saveEntry').each(function () {
    AJS.$(this).click();
  });
  addOrModifyStatusEntry('', '', null);
}

function getStatusChoices(id) {
  return '<td style="max-width: 16%">' + getRadioChoiceForStatus(id, 'default') + '<span class="aui-lozenge">&nbsp;&nbsp;&nbsp;&nbsp;</span></input></td>' +
    '<td style="max-width: 16%">' + getRadioChoiceForStatus(id, 'success') + '<span class="aui-lozenge aui-lozenge-success">&nbsp;&nbsp;&nbsp;&nbsp;</span></input></td>' +
    '<td style="max-width: 16%">' + getRadioChoiceForStatus(id, 'error') + '<span class="aui-lozenge aui-lozenge-error">&nbsp;&nbsp;&nbsp;&nbsp;</span></input></td>' +
    '<td style="max-width: 16%">' + getRadioChoiceForStatus(id, 'current') + '<span class="aui-lozenge aui-lozenge-current">&nbsp;&nbsp;&nbsp;&nbsp;</span></input></td>' +
    '<td style="max-width: 16%">' + getRadioChoiceForStatus(id, 'complete') + '<span class="aui-lozenge aui-lozenge-complete">&nbsp;&nbsp;&nbsp;&nbsp;</span></input></td>' +
    '<td style="max-width: 16%">' + getRadioChoiceForStatus(id, 'moved') + '<span class="aui-lozenge aui-lozenge-moved">&nbsp;&nbsp;&nbsp;&nbsp;</span></input></td>';
}

