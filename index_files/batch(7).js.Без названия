  var output = '<table class="gliffy-list-search-results">' + ((opt_data.index == 0) ? '<thead><tr><th class="gliffy-table-column-diagram"><div class="gliffy-background-f0"></div></th><th class="gliffy-table-column-space">' + soy.$$escapeHtml('\u041c\u0435\u0441\u0442\u043e') + '</th><th class="gliffy-table-column-date">' + soy.$$escapeHtml('\u041f\u043e\u0441\u043b\u0435\u0434\u043d\u0435\u0435 \u0438\u0437\u043c\u0435\u043d\u0435\u043d\u0438\u0435') + '</th></tr></thead>' : '') + '<tbody>';
  if (opt_data.diagrams.length > 0) {
    var diagramList112 = opt_data.diagrams;
    var diagramListLen112 = diagramList112.length;
    for (var diagramIndex112 = 0; diagramIndex112 < diagramListLen112; diagramIndex112++) {
      var diagramData112 = diagramList112[diagramIndex112];
